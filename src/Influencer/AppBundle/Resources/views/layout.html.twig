<!DOCTYPE html>
<html data-ng-app="app" ng-controller="AppCtrl">
<head>
	<meta http-equiv="content-type" content="text/html;charset=UTF-8">
	<meta charset="utf-8">
	<title>Influencer</title>
	<meta http-equiv="X-UA-Compatible" content="IE=edge">
	<meta name="viewport" content="width=device-width, initial-scale=1.0, maximum-scale=1.0, user-scalable=no, shrink-to-fit=no">
	{% block apple_touch_icon %}
	<link rel="apple-touch-icon" href="{{ asset('ico/60.png') }}">
    <link rel="apple-touch-icon" sizes="76x76" href="{{ asset('ico/76.png') }}">
    <link rel="apple-touch-icon" sizes="120x120" href="{{ asset('ico/120.png') }}">
    <link rel="apple-touch-icon" sizes="152x152" href="{{ asset('ico/152.png') }}">
	{% endblock %}
	<meta name="apple-mobile-web-app-capable" content="yes">
    <meta name="apple-touch-fullscreen" content="yes">
    <meta name="apple-mobile-web-app-status-bar-style" content="default">
    <meta content="{{ '{{app.description}}' }}" name="description">
    <meta content="{{ '{{app.author}}' }}" name="author">
    <link href="//code.ionicframework.com/ionicons/2.0.1/css/ionicons.min.css" rel="stylesheet">
	{% block stylesheets_vendor %}
	<link href="{{ asset('assets/plugins/pace/pace-theme-flash.css') }}" rel="stylesheet" type="text/css" />
    <link href="{{ asset('assets/plugins/boostrapv3/css/bootstrap.min.css') }}" rel="stylesheet" type="text/css" />
    <link href="{{ asset('assets/plugins/font-awesome/css/font-awesome.css') }}" rel="stylesheet" type="text/css" />
    <link href="{{ asset('assets/plugins/jquery-scrollbar/jquery.scrollbar.css') }}" rel="stylesheet" type="text/css" media="screen" />
	{% endblock %}
	<link id="lazyload_placeholder">
	{% block stylesheets %}
	<link href="{{ asset('css/pages-icons.css') }}" rel="stylesheet" type="text/css">
	<link href="{{ asset('node_modules/flag-icon-css/css/flag-icon.min.css') }}" rel="stylesheet" type="text/css">
    <link class="main-stylesheet" ng-href="{{ asset('css/pages.min.css') }}" rel="stylesheet" type="text/css" />
    <link href="{{ asset('bundles/influencerapp/css/styles.css') }}" rel="stylesheet" type="text/css" media="screen" />
    <!--[if lte IE 9]>
        <link href="pages/css/ie9.css" rel="stylesheet" type="text/css" />
    <![endif]-->
    <!--[if lt IE 9]>
            <link href="{{ asset('assets/plugins/mapplic/css/mapplic-ie.css') }}" rel="stylesheet" type="text/css" />
    <![endif]-->
	{% endblock %}
	<script type="text/javascript">
    window.onload = function() {
        // fix for windows 8
        if (navigator.appVersion.indexOf("Windows NT 6.2") != -1)
            document.head.innerHTML += '<link rel="stylesheet" type="text/css" href="pages/css/windows.chrome.fix.css" />'
    }
    </script>
</head>

<body class="fixed-header" ng-class="{
'bg-master-lighter': is('app.extra.timeline'),
'no-header': is('app.social') || is('app.calendar') || is('app.maps.vector') || is('app.maps.google'),
'menu-pin' : app.layout.menuPin || is('boxed'),
'menu-behind' : app.layout.menuBehind,
'box-layout' : is('boxed'),
'horizontal-menu': is('app.layouts.horizontal'),
'rtl' : is('app.layouts.rtl')
 }">
	{% block contents %}
    <div class="full-height" ui-view></div>
    {% endblock %}
    
    <script src="{{ asset('bundles/fosjsrouting/js/router.js') }}"></script>
    <script src="{{ path('fos_js_routing_js', { callback: 'fos.Router.setData' }) }}"></script>
    
    {% block javascripts_jquery %}
    <script src="{{ asset('assets/plugins/pace/pace.min.js') }}" type="text/javascript"></script>
    <!--script src="{{ asset('assets/plugins/jquery/jquery-1.11.1.min.js') }}" type="text/javascript"></script-->
    <script src="{{ asset('node_modules/jquery/dist/jquery.min.js') }}" type="text/javascript"></script>
    <script src="{{ asset('assets/plugins/modernizr.custom.js') }}" type="text/javascript"></script>
    <script src="{{ asset('assets/plugins/jquery-ui/jquery-ui.min.js') }}" type="text/javascript"></script>
    <script src="{{ asset('assets/plugins/boostrapv3/js/bootstrap.min.js') }}" type="text/javascript"></script>
    <script src="{{ asset('assets/plugins/jquery/jquery-easy.js') }}" type="text/javascript"></script>
    <script src="{{ asset('assets/plugins/jquery-unveil/jquery.unveil.min.js') }}" type="text/javascript"></script>
    <script src="{{ asset('assets/plugins/jquery-bez/jquery.bez.min.js') }}"></script>
    <script src="{{ asset('assets/plugins/jquery-ios-list/jquery.ioslist.min.js') }}" type="text/javascript"></script>
    <script src="{{ asset('assets/plugins/jquery-actual/jquery.actual.min.js') }}"></script>
    <script src="{{ asset('assets/plugins/jquery-scrollbar/jquery.scrollbar.min.js') }}"></script>
    <script src="{{ asset('assets/plugins/classie/classie.js') }}" type="text/javascript"></script>
    <script src="{{ asset('node_modules/datatables.net/js/jquery.dataTables.js') }}" type="text/javascript"></script>
	{% endblock %}
	
	{% block javascripts_angular %}
	<script src="{{ asset('node_modules/angular/angular.js') }}" type="text/javascript"></script>
	<script src="{{ asset('node_modules/angular-animate/angular-animate.min.js') }}"></script>
    <script src="{{ asset('node_modules/angular-messages/angular-messages.min.js') }}" type="text/javascript"></script>
    <script src="{{ asset('node_modules/angular-resource/angular-resource.min.js') }}" type="text/javascript"></script>
    <script src="{{ asset('node_modules/angular-sanitize/angular-sanitize.min.js') }}" type="text/javascript"></script>
    <script src="{{ asset('node_modules/angular-ui-router/release/angular-ui-router.min.js') }}" type="text/javascript"></script>
    <script src="{{ asset('assets/plugins/angular-ui-util/ui-utils.js') }}" type="text/javascript"></script>
    <script src="{{ asset('node_modules/angular-local-storage/dist/angular-local-storage.min.js') }}" type="text/javascript"></script>
    <script src="{{ asset('assets/plugins/angular-ui-select/select.min.js') }}" type="text/javascript"></script>
    <script src="{{ asset('assets/plugins/angular-oc-lazyload/ocLazyLoad.min.js') }}" type="text/javascript"></script>
    <script src="{{ asset('assets/plugins/ng-flow/dist/ng-flow-standalone.min.js') }}" type="text/javascript"></script>
    <script src="{{ asset('node_modules/angular-toastr/dist/angular-toastr.tpls.js') }}" type="text/javascript"></script>
    <script src="{{ asset('node_modules/satellizer/dist/satellizer.min.js') }}" type="text/javascript"></script>
    <script src="{{ asset('node_modules/angular-masonry/angular-masonry.min.js') }}" type="text/javascript"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/angular-datatables/0.6.0/angular-datatables.min.js" type="text/javascript"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/angular-datatables/0.6.0/plugins/bootstrap/angular-datatables.bootstrap.min.js" type="text/javascript"></script>
    {% endblock %}
    
    {% block javascripts_app %}
    <script src="{{ asset('pages/js/pages.js') }}" type="text/javascript"></script>
    <script src="{{ asset('bundles/influencerapp/js/app.js') }}" type="text/javascript"></script>
    <script src="{{ asset('bundles/influencerapp/js/config.js') }}" type="text/javascript"></script>
    <script src="{{ asset('bundles/influencerapp/js/config.lazyload.js') }}" type="text/javascript"></script>
    <script src="{{ asset('bundles/influencerapp/js/directives/passwordStrength.js') }}" type="text/javascript"></script>
    <script src="{{ asset('bundles/influencerapp/js/directives/passwordMatch.js') }}" type="text/javascript"></script>
    <script src="{{ asset('bundles/influencerapp/js/directives/checkForUnique.js') }}" type="text/javascript"></script>
    <script src="{{ asset('bundles/influencerapp/js/controllers/header.js') }}" type="text/javascript"></script>
    <script src="{{ asset('bundles/influencerapp/js/controllers/sidebar.js') }}" type="text/javascript"></script>
    <script src="{{ asset('bundles/influencerapp/js/controllers/search.js') }}" type="text/javascript"></script>
    <script src="{{ asset('bundles/influencerapp/js/controllers/login.js') }}" type="text/javascript"></script>
    <script src="{{ asset('bundles/influencerapp/js/controllers/signup.js') }}" type="text/javascript"></script>
    <script src="{{ asset('bundles/influencerapp/js/controllers/logout.js') }}" type="text/javascript"></script>
    <script src="{{ asset('bundles/influencerapp/js/services/account.js') }}" type="text/javascript"></script>
    {% endblock %}
    
    <script src="{{ asset('assets/js/directives/pg-sidebar.js') }}" type="text/javascript"></script>
    <script src="{{ asset('assets/js/directives/cs-select.js') }}" type="text/javascript"></script>
    <script src="{{ asset('assets/js/directives/pg-dropdown.js') }}" type="text/javascript"></script>
    <script src="{{ asset('assets/js/directives/pg-form-group.js') }}" type="text/javascript"></script>
    <script src="{{ asset('assets/js/directives/pg-navigate.js') }}" type="text/javascript"></script>
    <script src="{{ asset('assets/js/directives/pg-portlet.js') }}" type="text/javascript"></script>
    <script src="{{ asset('assets/js/directives/pg-tab.js') }}" type="text/javascript"></script>
    <script src="{{ asset('assets/js/directives/pg-search.js') }}" type="text/javascript"></script>
    <script src="{{ asset('assets/js/directives/pg-quickview.js') }}" type="text/javascript"></script>
    <script src="{{ asset('assets/js/directives/pg-notification-center.js') }}" type="text/javascript"></script>
    <script src="{{ asset('assets/js/directives/pg-horizontal-menu.js') }}" type="text/javascript"></script>
    <script src="{{ asset('assets/js/directives/pg-tab-dropdownfx.js') }}" type="text/javascript"></script>
    <script src="{{ asset('assets/js/directives/skycons.js') }}" type="text/javascript"></script>
</body>
</html>